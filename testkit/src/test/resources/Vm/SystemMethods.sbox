vm:
  stack:
    [utf8.TestSystemMethods]
  storage:
    utf8.init: "null"
dotnet-compilation:
  steps:
  - target: Pravda.dll
    sources:
    - PravdaDotNet/Pravda/Pravda.cs
    optimize: true
  - target: SystemMethods.exe
